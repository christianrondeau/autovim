#!/usr/bin/env bash
ParamsArray=''
for i in "$@"; do 
  i="${i//\\/\\\\}"
  ParamsArray="$ParamsArray\"${i//\"/\\\"}\","
done
VimCmdScript="let g:autovim_cmd=[$ParamsArray\"\"]"
VimLaunch="exec vim -u ${0%/*}/../src/vimrc.vim --not-a-term -c '$VimCmdScript' -c 'source ${0%/*}/../src/autovim.vim'"
eval "$VimLaunch"
